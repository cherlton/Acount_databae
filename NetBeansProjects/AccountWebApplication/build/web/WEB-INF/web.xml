<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>AddAccountServlet</servlet-name>
        <servlet-class>za.ac.tut.web.AddAccountServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchAccountServlet</servlet-name>
        <servlet-class>za.ac.tut.web.SearchAccountServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchAccountServlet1</servlet-name>
        <servlet-class>za.ac.tut.web.SearchAccountServlet1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetAllAccountsServlet</servlet-name>
        <servlet-class>za.ac.tut.web.GetAllAccountsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddAccountServlet</servlet-name>
        <url-pattern>/AddAccountServlet.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchAccountServlet</servlet-name>
        <url-pattern>/SearchAccountServlet.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchAccountServlet1</servlet-name>
        <url-pattern>/SearchAccountServlet1.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetAllAccountsServlet</servlet-name>
        <url-pattern>/GetAllAccountsServlet.do</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
